{
	//General configuration for index
	"general":{
		"cache_tokens" : 100,
		"index" : "wikipedia_index",
		//Default configuration for field types, if any value in *.fields is ommited, it's used from this configuration 
		"default" : {
			"string" : {},				
			"boolean" : {"tokens" : "TICK_TOCK"},
			"integer" : {"from" : 0, "to" : 100, "step" : 1, "strategy" : "sequence"},
			"date" : {"from" : "1970-01-01T00:00:00", "to" : "now", "step" : "1 Minutes", "strategy" : "sequence"}
		}
	},
	"histogram" : [
		 {
		 	"index" : "NAME",
		 	"type" : "TYPE",
		 	"fields" : {
			//Generate strings containing 1 or 2 words from defined vocabulary called "tokens"
				"title" : [
					//Produces 2 string array whose values will have 2 tokens(words)
					{"type": "string", "array": 3, "words" : 2, "tokens" : {"A::3135;;B::3000;;C::594"}},
					//Produces single string whose values will have 3 tokens(words)
					{"type": "string", "words" : 3, "tokens" : {"A::3135;;B::3000;;C::594"}}
				],
				"category" : {"type": "string", "words" : 1, "tokens" : {"Literature::50;;Arithmetic::20"}},
				"visits" : [
					{"type" : "integer", "from" : 0, "to" : 100, "step" : 1, "strategy" : "sequence"},
					{"type" : "integer", "from" : 0, "to" : 100, "strategy" : "random"},				
				],
				"visits_custom" : [ {"to" : 100}, {"from" : 500, "to" : 1000, "step" : 5}]
				//Generate dates in custom range where gap between dates are 5 minutes
				"published" : [
					{"type" : "date", "from" : "2015-01-01T00:00:00", "to" : "2015-01-10T00:00:00", "step" : "5 Minutes"},
					{"type" : "date", "from" : "2016-01-01T00:00:00", "to" : "2016-01-04T00:00:00", "step" : "5 Minutes"},	
				],
				"controversial" : [
					{"type" : "boolean", "tokens" : "TRUE"},
					{"type" : "boolean", "tokens" : "TICK_TOCK"},
					//In 100 values 80 will be set to true and 20 to false 
					{"type" : "boolean", "tokens" : { "true" : 80, "false" : 20}},	
				],
				//Generate IP addresses using CIDR notation
				"ipv4" : {"type" : "ipv4", "cidr": "127.0.0.1/30"},
				"ipv4_multiple" : [
					{"type" : "ipv4",  "cidr" : "127.0.0.0/30"},
					{"type" : "ipv4",  "cidr" : "192.168.0.0/30"}	
				]
				
				//Objects and nested
				"authors.name" : [
					{"type": "string", "words" : 2, "tokens" : {"Rod::3135;;Johnson::3000;;Joshua::600;;Bloch::594"}},
					{"type": "string", "words" : 2, "tokens" : {"James::3135;;Gosling::3000"}}
				],
				"authors.age" : {"from" : 25, "to" : 70, "step" : 5},
				"authors.project.name" : {"type": "string", "words" : 2, "tokens" : {"ElasticSearch::3135;;Hibernate::3000;;Lucene::2000;;JNA::1000"}},
				"authors.project.contributors" : {"type" : "integer", "from" : 0, "to" : 100, "step" : 1, "strategy" : "sequence"},
				"authors.project.commits" : {"type" : "long", "from" : 100, "to" : 100000, "step" : 50},
			}
		}
	]		
}