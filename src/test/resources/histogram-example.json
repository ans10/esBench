{
	//General configuration for index
	"general":{
		"cache_tokens" : 100,
		"index" : "wikipedia_index",
		//Default configuration for field types, if any value in *.fields is ommited, it's used from this configuration 
		"default" : {
			"String" : {"strategy" : "sequence"},			
			"Boolean" : {"TICK_TOCK"},
			"Integer" : {"from" : 0, "to" : 100, "increment" : 1, "strategy" : "sequence"},
			"Date" : {"from" : "1970-01-01T00:00:00", "to" : "now", "increment" : "1 Minutes", "strategy" : "sequence"}
		}
	},
	
	//Concrete fields are named as *.fields
	//=====================================
	
	//Generate strings containing 1 or 2 words from defined vocabulary called "tokens"
	"String.fields" : {
		"title" : {"words" : 2, "tokens" : {"A::3135;;B::3000;;C::594"}},
		"category" : {"words" : 1, "tokens" : {"Literature::50;;Arithmetic::20"}},
		"contributors.name" : {"words" : 1, "tokens" : {"Bill::10;;Alex::5"}},
	},
	//Alternative to "contributors.name"
	"Object.fields" : {
		"" : {
			"String.fields" : {},
			"Integer.fields" : {}
		}
	},
	"Integer.fields" : {
		"visits" : {"from" : 0, "to" : 100, "increment" : 1, "strategy" : "sequence"},
		"visits_random" : {"from" : 0, "to" : 100, "strategy" : "random"},
		"visits_custom" : [ {"to" : 100}, {"from" : 500, "to" : 1000, "increment" : 5}, {}]
	},
	//Generate dates in custom range where gap between dates are 5 minutes
	"Date.fields" : {
		"published" : {"from" : "2015-01-01T00:00:00", "to" : "2015-01-10T00:00:00", "increment" : "5 Minutes"},
		"published" : {"from" : "2015-01-01T00:00:00", "to" : "2015-01-10T00:00:00", "increment" : "5 Minutes"},
		"published_alternative" : {"range": "<2015-01-01T00:00:00,2015-01-10T00:00:00>", "increment" : "5 Minutes"},
		
	},
	"Boolean.fields" : {
		"controversial" : {"TRUE"},
		"controversial_other" : {"TICK_TOCK"},
		//In 100 values 80 will be set to true and 20 to false 
		"controversial_alternative" : { "true" : 80, "false" : 20},
	},
	//Generate IP addresses using CIDR notation
	"IP.fields" : {
		"ipv4" : {"127.0.0.1/30"},
		"ipv4_multiple" : ["127.0.0.0/30", "192.168.0.0/30"]
	}
	
}